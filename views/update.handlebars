

<div class="container d-flex justify-content-center p-5 mt-5">
    <div class="d-flex justify-content-center h-100">
         <div class="card" style=" background-color: transparent">
              <div class="card-header d-flex justify-content-center">
				<h3 style="color: aliceblue;">Register</h3>
			</div>
            <div class="card-body">
                {{#each doc}}
                <form action="/admin/upload/{{this._id}}" method="POST" autocomplete="off">
                        
                     <div class="form-group">
                         <label for="username" style="color: aliceblue;">UserName</label>
                        <input type="text" id="username" name="username" required placeholder="Enter UserName" value="{{this.username}}" class="form-control" style=" background-color: transparent "> 
                    </div>
                     <div class="form-group">
                    <label for="phone" style="color: aliceblue;">Mobile No:</label>
                        <input type="number"  name="phone" id="phone" required class="form-control" placeholder="Mobile Number" value="{{this.phone}}" style=" background-color: transparent "> 
                    </div>
                    {{!-- <div class="form-group">
                        <label for="password" style="color: aliceblue;">Password:</label>
                        <input type="password" id="password" name="password" placeholder="Enter Password" class="form-control" value="{{this.password}}" required style=" background-color: transparent " maxlength="6"> 
                    </div> --}}
                    <div class="form-group">
                   <input type="submit" class="btn btn-warning btn-block float-right login_btn" value="UPDATE">
                     
					</div>           
                </form>
                    {{/each}}
            </div>
         </div>
    </div>
</div>







<script>
   function preback(){window.history.forward();}
   setTimeout("preback()",0);
   window.onunload=function(){null};
 </script>



